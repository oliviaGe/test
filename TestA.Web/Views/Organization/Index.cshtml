@using MetaShare.Common.Core.Presentation
@using TestA.Entities
@using TestA.Web.HtmlExtensions
@using TestA.Web.Models

@model IndexViewModel<TestA.Entities.Organization>
@{
    ViewBag.Title = "Index";
    Layout  = "~/Views/Shared/_Layout.cshtml";
}

<div class="m_title_box">
    <ol>
        <li data-id="Organization">
            <a href="#">Organization</a><i class="icon-arrowright"></i>
        </li>
        <li data-id="Index">Index</li>
    </ol>
    <div class="m_title_action">
        <a class="m_btn" href="@(this.Url.Action("Create", "Organization", null))">New</a>
    </div>
</div>
<div class="panel panel-default">
    
    <div class="panel-heading">Organization</div>
    <div class="panel-body">
            @Html.Partial(Url.Content("_Search"), Model.SearchModel)
    <div class="table-responsive" id="Organization_DataList">
            @Html.Partial(Url.Content("_DataList"), Model)
    </div>
    </div>
</div>
@this.Html.Partial("_DeleteDialog")
